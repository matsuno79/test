
public class Student {
	String name;
	int score;
	static final int MAX_SCORE = 100;


	String getName(){
		return name;
	}

	void setName(String name){
		this.name = name;
	}

	int getScore() {
		return score;
	}

void setScore(int score) {
	this.score = score;
}


void printScore(int maxScore) {
	System.out.println(name+"さんは"+MAX_SCORE+"点満点中"+score+"点です");
}


}
