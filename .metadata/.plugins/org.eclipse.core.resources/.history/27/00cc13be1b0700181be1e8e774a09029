public class Person {
	private String a = null;
	private int b = 0;
	private String c = null;


	public Person(String a,int b,String c) {
		this.a = a;
		this.b = b;
		this.c = c;
	}


	public String getA() {
		return this.a;
	}

	public void setA(String a) {
		this.a = a;
	}

	public int getB() {
		return this.b;
	}

	public void setB(int b) {
		this.b = b;

}

}