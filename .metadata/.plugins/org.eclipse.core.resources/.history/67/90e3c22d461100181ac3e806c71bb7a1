
public class Student {
	String name;
	int score;
	static final int MAX_SCORE = 100;


	Student(String name,int score){
		this.name = name;
		this.score = score;
	}

	Student(String name){
		this(name,0);
	}



void printScore(int maxScore) {
	System.out.println(name+"さんは"+MAX_SCORE+"点満点中"+score+"点です");
}


}
