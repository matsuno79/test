<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN2"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Struts詳細設定-->

<struts>
	<constant name="struts.devMode"value="true"/>
	<!--対象のパッケージ詳細設定 -->
	<package name="com.internousdev.template.actiom"extends="struts-default">

	<!-- HomeAction -->
		<actiom name="HomeAction"class"com.internousdev.template.action.HomeAction"method="execute">
			<result name ="success">login.jsp</result>
		</actiom>

	<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.template.action.LoginAction"method="execute">
			<result name ="success">buyItem.jsp</result>
			<result name ="error">home.jsp</result>
		</action>

	<!-- UserCreateActiom -->
	<action name="UserCreateAction"class="com.internousdev.template.action.UserCreateAction" method="execute">
		<result name="success">userCreate.jsp</result>
		<result name ="back">login.jsp</result>
	</action>

</package>
</struts>