
public class Student {
	String name;
	int score;
	static final int MAX_SCORE = 100;


	int getScore() {
		return score;
	}

void setScore(int newScore) {
	score = newScore;
}

void printScore(){
	printScore(MAX_SCORE);
}


void printScore(int maxScore) {
	System.out.println(name+"さんは"+MAX_SCORE+"点満点中"+score+"点です");
}


}
