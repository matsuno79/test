
public class Helloworld {

	public static void main(String[] args){

		int nun = 88;


		if(nun <= 100){

			if(nun < 20) {
				System.out.println("未成年");



			}


			else if(nun == 77){
				System.out.println("喜寿");
			}

			else if(nun == 88){
				System.out.println("米寿");
			}
			else{
				System.out.println("成人");
			}

		}



	}
}
